{
    "name": "egately/egate-otp",
    "description": "OTP Mangement Package",
    "keywords": [
        "egate",
        "laravel",
        "egate-otp"
    ],
    "homepage": "https://github.com/egately/egate-otp",
    "license": "MIT",
    "authors": [
        {
            "name": "Salah Elabbar",
            "email": "info@egate.ly",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "bacon/bacon-qr-code": "^2.0|^3.0",
        "illuminate/support": "^8.82|^9.0|^10.0|^11.0",
        "pragmarx/google2fa": "^7.0|^8.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Egate\\EgateOtp\\": "src/",
            "Egate\\EgateOtp\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Egate\\EgateOtp\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Egate\\EgateOtp\\EgateOtpServiceProvider"
            ],
            "aliases": {
                "EgateOtp": "Egate\\EgateOtp\\Facades\\EgateOtp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
